  },
  "scripts": {
    "start": "start-storybook -p 9009 -s public",
    "test": "jest",
    "echo": "echo $#",
    "test-update": "jest -u",
    "lint": "eslint src",
    "build": "webpack --config .storybook/webpack.config.js",
    "pub-patch": "npm ls && npm run lint && npm run test && npm run build && npm version patch && npm publish .",
    "pub-minor": "npm ls && npm run lint && npm run test && npm run build && npm version minor && npm publish .",
    "pub-major": "npm ls && npm run lint && npm run test && npm run build && npm version major && npm publish .",
    "storybook": "start-storybook -p 6006 -s .storybook/public",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.less$": "<rootDir>/jest-processor.js"
    }
  },
  "main": "build/index.js",
  "files": [
    "build"
  ]
